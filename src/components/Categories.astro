---
var caterogyList = [
    'Electronic Devices',
    'Computer & Laptop',
    'Computer Accessories',
    'SmartPhone',
    'HeadPhone',
    'Mobile Accessories',
    'Gaming Console',
    'Camera & Photo',
    'TV & Home Appliances',
    'Watches & Accessories',
    'GPS & Navigation',
    'Warable Technology'
]

---
<div class="w-full bg-white py-2 flex items-center">
    <div class="container mx-auto px-4 flex items-center justify-between">
        <div class="flex items-center">
            <button id="dropdownDefaultButton" data-dropdown-toggle="category" class="flex items-center bg-[#F2F4F5] px-10 py-3 font-medium" type="button">All Category 
                <svg class="w-2.5 h-2.5 ms-3" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 10 6">
                <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="m1 1 4 4 4-4"/>
                </svg>
                </button>
                
                <!-- Dropdown menu -->
                <div id="category" class="z-10 hidden bg-[#f2f4f5] divide-y divide-gray-100 shadow w-44 dark:bg-gray-700">
                    <ul class="py-2 text-sm text-gray-700 dark:text-gray-200" aria-labelledby="dropdownDefaultButton">
                        {
                            caterogyList.map((item) => (
                            <li>
                                <a href="#" class="block px-4 py-2 hover:bg-gray-200 dark:hover:bg-gray-600 dark:hover:text-white">{ item}</a>
                            </li>
                            ))
                        }
                      
    
                      
                    </ul>
                </div>  
 
    

    <div class="flex items-center mx-3 gap-x-5">
        <span class="flex items-center text-[#5F6C72] cursor-pointer">
            <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-6 mx-1">
                <path stroke-linecap="round" stroke-linejoin="round" d="M15 10.5a3 3 0 1 1-6 0 3 3 0 0 1 6 0Z" />
                <path stroke-linecap="round" stroke-linejoin="round" d="M19.5 10.5c0 7.142-7.5 11.25-7.5 11.25S4.5 17.642 4.5 10.5a7.5 7.5 0 1 1 15 0Z" />
              </svg>
              Track Order
        </span>

        <span class="flex items-center text-[#5F6C72] cursor-pointer">
            <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-6 mx-1">
                <path stroke-linecap="round" stroke-linejoin="round" d="M16.023 9.348h4.992v-.001M2.985 19.644v-4.992m0 0h4.992m-4.993 0 3.181 3.183a8.25 8.25 0 0 0 13.803-3.7M4.031 9.865a8.25 8.25 0 0 1 13.803-3.7l3.181 3.182m0-4.991v4.99" />
              </svg>
              
              Compare
        </span>

        <span class="flex items-center text-[#5F6C72] cursor-pointer">
            <svg xmlns="http://www.w3.org/2000/svg" class="size-6 mx-1" viewBox="0 0 24 24">
                <g fill="none">
                    <path stroke="currentColor" stroke-linecap="round" stroke-width="1.5" d="M21 15v-3a9 9 0 1 0-18 0v3" />
                    <path fill="currentColor" d="M3 15v-.75h-.75V15zm0 .75h3v-1.5H3zm3.25.25v3h1.5v-3zm-2.5 3v-4h-1.5v4zM5 20.25c-.69 0-1.25-.56-1.25-1.25h-1.5A2.75 2.75 0 0 0 5 21.75zM6.25 19c0 .69-.56 1.25-1.25 1.25v1.5A2.75 2.75 0 0 0 7.75 19zM6 15.75a.25.25 0 0 1 .25.25h1.5A1.75 1.75 0 0 0 6 14.25zM21 15h.75v-.75H21zm-3 .75h3v-1.5h-3zm2.25-.75v4h1.5v-4zm-2.5 4v-3h-1.5v3zM19 20.25c-.69 0-1.25-.56-1.25-1.25h-1.5A2.75 2.75 0 0 0 19 21.75zM20.25 19c0 .69-.56 1.25-1.25 1.25v1.5A2.75 2.75 0 0 0 21.75 19zM18 14.25A1.75 1.75 0 0 0 16.25 16h1.5a.25.25 0 0 1 .25-.25z" />
                </g>
            </svg>
              Customer Support
        </span>

        <span class="flex items-center text-[#5F6C72] cursor-pointer">
            <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-6 mx-1">
                <path stroke-linecap="round" stroke-linejoin="round" d="m11.25 11.25.041-.02a.75.75 0 0 1 1.063.852l-.708 2.836a.75.75 0 0 0 1.063.853l.041-.021M21 12a9 9 0 1 1-18 0 9 9 0 0 1 18 0Zm-9-3.75h.008v.008H12V8.25Z" />
              </svg>
              
              Need Help
        </span>
    </div>
</div>

   <div class="flex items-center gap-x-2">
     <a href="tel:+1-202-555-0104" class="text-gray-900 flex items-center gap-x-2 font-medium">
        <img src="/images/Phone.svg">
        +1-202-555-0104
     </a>
   </div>

  </div>
</div>

