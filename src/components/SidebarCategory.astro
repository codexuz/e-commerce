---
var caterogyLists = [
    'Electronic Devices',
    'Computer & Laptop',
    'Computer Accessories',
    'SmartPhone',
    'HeadPhone',
    'Mobile Accessories',
    'Gaming Console',
    'Camera & Photo',
    'TV & Home Appliances',
    'Watches & Accessories',
    'GPS & Navigation',
    'Warable Technology'
]
---
<div class="w-full px-1">
    <h1 class="text-lg mb-3 font-medium uppercase">Category</h1>
    {
        caterogyLists.map((item, index) => (
        <div class="flex items-center mb-4">
            <input id={index} type="radio" value="" name="default-radio" class="w-5 h-5 text-[#FA8232] bg-gray-100 border-gray-300 focus:ring-[#FA8232] dark:focus:ring-blue-600 dark:ring-offset-gray-800 focus:ring-2 dark:bg-gray-700 dark:border-gray-600">
            <label for={index} class="ms-2 text-sm  text-gray-900 dark:text-gray-300">{item}</label>
        </div>
        ))
    }
</div>

